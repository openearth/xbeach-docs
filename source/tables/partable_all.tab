     :units:         
     :default:       roelvink_total
     :range:         none, roelvink_total, roelvink_bed, soulsby_total, soulsby_bed
   bed
     :advanced:
     :description:   Calibration factor for bed transports
     :units:         -
     :default:       1
     :range:         0 - 1
   bedfriccoef
     :description:   Bed friction coefficient
     :units:         -
     :default:       0.01
     :range:         3.5e-05 - 0.9
   bedfricfile
     :description:   Bed friction file (valid for all bed friction coefficients)
     :units:         <file>
   bedfriction
     :description:   Bed friction formulation
     :units:         
     :default:       chezy
     :range:         chezy, cf, white-colebrook, manning, white-colebrook-grainsize
   bermslope
     :description:   Swash zone slope for (semi-) reflective beaches
     :units:         -
     :default:       0.0
     :range:         0.0 - 1.0
