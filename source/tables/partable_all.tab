     :units:         -
     :default:       0.025
     :range:         0.001 - 1.0
   depfile
     :required:
     :required:
     :required:
     :description:   Name of the input bathymetry file
     :units:         <file>
   depthscale
     :advanced:
     :description:   Depthscale of (lab)test simulat, affects eps, hmin, hswitch and dzma
     :units:         -
     :default:       1.0
     :range:         1.0 - 200.0
   dilatancy
     :description:   Switch to reduce critical shields number due dilatancy
     :units:         -
     :default:       0
     :range:         0 - 1
   dir0
     :description:   Mean wave direction for instat = stat, bichrom, ts_1 or ts_2 (nautical convention)
     :units:         deg
     :default:       270.0
     :range:         -360.0 - 360.0
   disch_loc_file
     :advanced:
     :description:   Name of discharge locations file
     :units:         <file>
   disch_timeseries_file
     :advanced:
     :description:   Name of discharge timeseries file
     :units:         <file>
   dispc
     :advanced:
     :description:   Coefficient in front of the vertical pressure gradient
