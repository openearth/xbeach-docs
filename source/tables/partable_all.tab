     :advanced:
     :description:   Breaker slope coefficient in roller model
     :units:         -
     :default:       0.1
     :range:         0.05 - 0.3
   betad
     :advanced:
     :description:   Dissipation parameter long wave breaking turbulence
     :units:         -
     :default:       1.0
     :range:         0.0 - 10.0
   break
     :description:   Type of breaker formulation
     :units:         
     :default:       roelvink2
     :range:         roelvink1, baldock, roelvink2, roelvink_daly, janssen
   breakerdelay
     :advanced:
     :description:   Switch to enable breaker delay model
     :units:         -
     :default:       1.0
     :range:         0.0 - 3.0
   breakviscfac
     :advanced:
     :description:   Factor to increase viscosity during breaking
     :units:         -
     :default:       1.5
     :range:         1.0 - 3.0
   breakvisclen
     :advanced:
     :description:   Ratio between local depth and length scale in extra breaking viscosity
     :units:         -
     :default:       1.0
     :range:         0.75 - 3.0
   bulk
     :advanced:
     :description:   Switch to compute bulk transport rather than bed and suspended load separately
     :units:         -
     :default:       0
     :range:         0 - 1
   cats
     :advanced:
     :description:   Current averaging time scale for wci, in terms of mean wave periods
     :units:         Trep
     :default:       4.0
     :range:         1.0 - 50.0
   ci
     :advanced:
     :description:   Mass coefficient in shields inertia term
     :units:         -
     :default:       1.0
     :range:         0.5 - 1.5
   cm
     :advanced,deprecated:
     :description:   Mass coefficient in shields inertia term
     :units:         -
     :default:       1.5
     :range:         0.0 - 3.0
   cmax
     :advanced:
     :description:   Maximum allowed sediment concentration
     :units:         -
     :default:       0.1
     :range:         0.0 - 1.0
   compi
     :description:   Imaginary unit
     :units:         -
     :default:       -123
   correctHm0
     :advanced,silent:
     :description:   Switch to enable hm0 correction
     :units:         -
     :default:       1
     :range:         0 - 1
   cyclic
     :description:   Turn on cyclic boundary conditions
     :units:         -
     :default:       0
     :range:         0 - 1
   defuse
     :advanced,silent:
     :description:   Turn on timestep explosion prevention mechanism
     :units:         -
     :default:       1
     :range:         0 - 1
   delta
     :advanced:
     :description:   Fraction of wave height to add to water depth
     :units:         -
     :default:       0.0
     :range:         0.0 - 1.0
   deltar
     :advanced,silent:
